
{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/BookStore.API/bin/Debug/netcoreapp2.2/BookStore.API.dll",
      "args": [],
      "cwd": "${workspaceFolder}/BookStore.API",
      "stopAtEntry": false,
      "requireExactSource": false,
      "preLaunchTask": "build",
      "launchBrowser": {
        "enabled": false
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}